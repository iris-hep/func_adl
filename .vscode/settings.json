{
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "python.testing.pytestEnabled": true,
    "cSpell.words": [
        "AST's",
        "asts",
        "astunparse",
        "asyncio",
        "backends",
        "bdist",
        "codecov",
        "consts",
        "databanks",
        "dataclass",
        "dataframes",
        "Dlib",
        "elts",
        "getenv",
        "getmro",
        "gwatts",
        "iscoroutine",
        "isfunction",
        "ismethod",
        "isort",
        "Jupyter",
        "kwonlyargs",
        "LINQ",
        "localds",
        "macos",
        "nargs",
        "noqa",
        "numpy",
        "orelse",
        "posonlyargs",
        "prereleased",
        "pypa",
        "pypi",
        "pytest",
        "qastle",
        "ROOTT",
        "rucio",
        "sdist",
        "servicex",
        "setuptools",
        "sourcelines",
        "syntatic",
        "Topo",
        "unittests",
        "unparse",
        "url's",
        "vararg",
        "xaod",
        "xrootd"
    ],
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/objects/**",
        ".vscode",
        "tests/**"
    ],
    "editor.formatOnSave": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.testing.pytestArgs": [
        "--no-cov"
    ],
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "python.formatting.provider": "black",
    "python.linting.flake8Args": [
        "--config=.flake8"
    ],
    "cmake.configureOnOpen": false
}